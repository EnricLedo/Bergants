<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout7"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".membres"
    android:padding="@dimen/MargePapereraEsquerra20dp">



    <TextView
        android:id="@+id/membres"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/marginpag"
        android:background="@color/grisClaretGentApuntada"
        android:gravity="center_horizontal"
        android:text="@string/membres"
        android:textSize="@dimen/TitolActuacio23sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="60dp"
        app:layout_constraintTop_toTopOf="parent"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerMembres"
        android:layout_width="match_parent"
        android:layout_height="640dp"
        app:layout_constraintTop_toBottomOf="@id/membres"
        tools:listitem="@layout/diseny_membre" />



    <Button
        android:id="@+id/botoAfegirMembre"
        android:layout_width="@dimen/btncircle"
        android:layout_height="@dimen/btncircle"
        android:background="@drawable/round_button"
        android:gravity="center"
        android:text="@string/afegir"
        android:textColor="@color/blanc"
        android:textSize="@dimen/txtcircle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>